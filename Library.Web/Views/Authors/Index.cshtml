@model IEnumerable<Library.Web.Data.Entities.Author>

@{
    ViewData["Title"] = "Index";
}

<h1>Autores</h1>

<div class="row">
    <div class="">
        <table class="table-responsive">    
            <thead>
                <tr>
                    <th>Nombres</th>
                    <th>Apellidos</th>
                </tr>
            </thead>

            <tbody>
                @foreach(var item in Model)
                {
                    <tr>
                        <td>@item.FirstName</td>
                        <td>@item.LastName</td>
                    </tr>
                }
            </tbody>

        </table>
    </div>
</div>




